xfout_hikey=e2
// mod wheel slows attack (PB)
ampeg_attackcc1=0.5

// randomize like a real player(PB)
pitch_random=12
amp_random=1.5
delay_random=0.012



// add some volume vibrato (PB)
amplfo_freq=4.57
amplfo_depth=0.6
amplfo_delay=1.0
