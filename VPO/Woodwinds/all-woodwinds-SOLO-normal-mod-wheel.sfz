// add some further volume vibrato [sfz2.0] (PB)
lfo01_delay=2.3
lfo01_fade=0.5
lfo01_wave=1
lfo01_volume=0.20
lfo01_freq=2.79

// longer attack for sustain (PB)
ampeg_attack=0.03

ampeg_release=0.500

