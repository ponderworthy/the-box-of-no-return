<group>
// sustain by default
sw_default=c2
sw_lokey=c2
sw_hikey=f2
sw_last=c2
sw_label=C2 Sustain

// randomize like a real player(PB)
pitch_random=12
amp_random=1.5
delay_random=0.012



ampeg_attack=0.6

// slower release for slower notes = legato effect (PB)
// ampeg_release=0.7
ampeg_release=1.7
ampeg_vel2release=-1.0

// adjust tone (PB)
eq1_freq=807.4
eq1_bw=1.54
eq1_gain=-9.2

eq2_freq=5303.4
eq2_bw=0.86
eq2_gain=-8.8

volume=4
 group=1  xfin_lovel=54
xfin_hivel=104
