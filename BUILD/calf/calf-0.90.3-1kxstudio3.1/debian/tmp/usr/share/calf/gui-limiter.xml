<table rows="20" cols="4">
    <align attach-x="0" attach-y="0"   align-x="1"><label param="bypass"  /></align>
    <align attach-x="1" attach-w="2" attach-y="0" expand-x="1" fill-x="1" pad-x="10" ><toggle param="bypass" /></align>

    <!-- -->

    <align attach-x="0" attach-y="1"   align-x="1"><label param="level_in"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="1" shrink-x="1" param="level_in"  />
    <value param="level_in"  attach-x="2" attach-y="1" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="2"   align-x="1"><label param="level_out"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="2" shrink-x="1" param="level_out"  />
    <value param="level_out"  attach-x="2" attach-y="2" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="3"   align-x="1"><label param="meter_inL"  /></align>
    <vumeter attach-x="1" attach-w="1" attach-y="3" expand-x="1" fill-x="1" param="meter_inL" />
    <value param="meter_inL"  attach-x="2" attach-y="3" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="4"   align-x="1"><label param="meter_inR"  /></align>
    <vumeter attach-x="1" attach-w="1" attach-y="4" expand-x="1" fill-x="1" param="meter_inR" />
    <value param="meter_inR"  attach-x="2" attach-y="4" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="5"   align-x="1"><label param="meter_outL"  /></align>
    <vumeter attach-x="1" attach-w="1" attach-y="5" expand-x="1" fill-x="1" param="meter_outL" />
    <value param="meter_outL"  attach-x="2" attach-y="5" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="6"   align-x="1"><label param="meter_outR"  /></align>
    <vumeter attach-x="1" attach-w="1" attach-y="6" expand-x="1" fill-x="1" param="meter_outR" />
    <value param="meter_outR"  attach-x="2" attach-y="6" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="7"   align-x="1"><label param="clip_inL"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="7" shrink-x="1" param="clip_inL"  />
    <value param="clip_inL"  attach-x="2" attach-y="7" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="8"   align-x="1"><label param="clip_inR"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="8" shrink-x="1" param="clip_inR"  />
    <value param="clip_inR"  attach-x="2" attach-y="8" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="9"   align-x="1"><label param="clip_outL"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="9" shrink-x="1" param="clip_outL"  />
    <value param="clip_outL"  attach-x="2" attach-y="9" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="10"   align-x="1"><label param="clip_outR"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="10" shrink-x="1" param="clip_outR"  />
    <value param="clip_outR"  attach-x="2" attach-y="10" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="11"   align-x="1"><label param="limit"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="11" shrink-x="1" param="limit"  />
    <value param="limit"  attach-x="2" attach-y="11" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="12"   align-x="1"><label param="attack"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="12" shrink-x="1" param="attack"  />
    <value param="attack"  attach-x="2" attach-y="12" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="13"   align-x="1"><label param="release"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="13" shrink-x="1" param="release"  />
    <value param="release"  attach-x="2" attach-y="13" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="14"   align-x="1"><label param="att"  /></align>
    <vumeter attach-x="1" attach-w="1" attach-y="14" expand-x="1" fill-x="1" param="att" mode="2"/>
    <value param="att"  attach-x="2" attach-y="14" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="15"   align-x="1"><label param="asc"  /></align>
    <align attach-x="1" attach-w="2" attach-y="15" expand-x="1" fill-x="1" pad-x="10" ><toggle param="asc" /></align>

    <!-- -->

    <align attach-x="0" attach-y="16"   align-x="1"><label param="asc_led"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="16" shrink-x="1" param="asc_led"  />
    <value param="asc_led"  attach-x="2" attach-y="16" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="17"   align-x="1"><label param="asc_coeff"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="17" shrink-x="1" param="asc_coeff"  />
    <value param="asc_coeff"  attach-x="2" attach-y="17" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="18"   align-x="1"><label param="oversampling"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="18" shrink-x="1" param="oversampling"  />
    <value param="oversampling"  attach-x="2" attach-y="18" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="19"   align-x="1"><label param="auto_level"  /></align>
    <align attach-x="1" attach-w="2" attach-y="19" expand-x="1" fill-x="1" pad-x="10" ><toggle param="auto_level" /></align>
    <if cond="directlink">
        <vbox expand-x="1" fill-x="1" attach-x="3" attach-y="0" attach-h="20">
            <line-graph refresh="1" width="160" param="att"/>

            <line-graph refresh="1" width="160" param="asc_coeff"/>

        </vbox>
    </if>
</table>

