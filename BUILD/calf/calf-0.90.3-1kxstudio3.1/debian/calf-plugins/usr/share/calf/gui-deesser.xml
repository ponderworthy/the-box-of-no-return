<table rows="17" cols="4">
    <align attach-x="0" attach-y="0"   align-x="1"><label param="bypass"  /></align>
    <align attach-x="1" attach-w="2" attach-y="0" expand-x="1" fill-x="1" pad-x="10" ><toggle param="bypass" /></align>

    <!-- -->

    <align attach-x="0" attach-y="1"   align-x="1"><label param="detected"  /></align>
    <vumeter attach-x="1" attach-w="1" attach-y="1" expand-x="1" fill-x="1" param="detected" />
    <value param="detected"  attach-x="2" attach-y="1" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="2"   align-x="1"><label param="compression"  /></align>
    <vumeter attach-x="1" attach-w="1" attach-y="2" expand-x="1" fill-x="1" param="compression" mode="2"/>
    <value param="compression"  attach-x="2" attach-y="2" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="3"   align-x="1"><label param="detected_led"  /></align>
    <led attach-x="1" attach-w="2" attach-y="3" param="detected_led" shrink-x="1" pad-x="10"  />

    <!-- -->

    <align attach-x="0" attach-y="4"   align-x="1"><label param="clip_out"  /></align>
    <led attach-x="1" attach-w="2" attach-y="4" param="clip_out" shrink-x="1" pad-x="10"  />

    <!-- -->

    <align attach-x="0" attach-y="5"   align-x="1"><label param="detection"  /></align>
    <combo attach-x="1" attach-w="2" attach-y="5" param="detection" expand-x="1" pad-x="10"  />

    <!-- -->

    <align attach-x="0" attach-y="6"   align-x="1"><label param="mode"  /></align>
    <combo attach-x="1" attach-w="2" attach-y="6" param="mode" expand-x="1" pad-x="10"  />

    <!-- -->

    <align attach-x="0" attach-y="7"   align-x="1"><label param="threshold"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="7" shrink-x="1" param="threshold"  />
    <value param="threshold"  attach-x="2" attach-y="7" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="8"   align-x="1"><label param="ratio"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="8" shrink-x="1" param="ratio"  />
    <value param="ratio"  attach-x="2" attach-y="8" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="9"   align-x="1"><label param="laxity"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="9" shrink-x="1" param="laxity"  />
    <value param="laxity"  attach-x="2" attach-y="9" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="10"   align-x="1"><label param="makeup"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="10" shrink-x="1" param="makeup"  />
    <value param="makeup"  attach-x="2" attach-y="10" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="11"   align-x="1"><label param="f1_freq"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="11" shrink-x="1" param="f1_freq"  />
    <value param="f1_freq"  attach-x="2" attach-y="11" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="12"   align-x="1"><label param="f2_freq"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="12" shrink-x="1" param="f2_freq"  />
    <value param="f2_freq"  attach-x="2" attach-y="12" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="13"   align-x="1"><label param="f1_level"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="13" shrink-x="1" param="f1_level"  />
    <value param="f1_level"  attach-x="2" attach-y="13" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="14"   align-x="1"><label param="f2_level"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="14" shrink-x="1" param="f2_level"  />
    <value param="f2_level"  attach-x="2" attach-y="14" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="15"   align-x="1"><label param="f2_q"  /></align>
    <knob attach-x="1" attach-w="1" attach-y="15" shrink-x="1" param="f2_q"  />
    <value param="f2_q"  attach-x="2" attach-y="15" pad-x="10" />

    <!-- -->

    <align attach-x="0" attach-y="16"   align-x="1"><label param="sc_listen"  /></align>
    <align attach-x="1" attach-w="2" attach-y="16" expand-x="1" fill-x="1" pad-x="10" ><toggle param="sc_listen" /></align>
    <if cond="directlink">
        <vbox expand-x="1" fill-x="1" attach-x="3" attach-y="0" attach-h="17">
            <line-graph refresh="1" width="160" param="compression"/>

            <line-graph refresh="1" width="160" param="f1_freq"/>

        </vbox>
    </if>
</table>

